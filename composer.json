{
	"name": "sabina-talipova/silverstripe-3d-scene",
	"description": "SilverStripe module for creating and working with 3D models",
	"type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "scene",
        "3d scene"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Sabina Talipova",
        "email": "sabina.talipova@silverstripe.com"
    }],
    "require": {
		"silverstripe/framework": "^5",
		"silverstripe/cms": "^5",
		"silverstripe/admin": "^2",
		"sabina-talipova/silverstripe-threejs": "1.x-dev"
	},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:sabina-talipova/silverstripe-threejs.git"
		}
	],
    "extra": {
        "installer-name": "3d-scene",
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {}
    },
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	}
}
